//https://www.codingninjas.com/studio/problems/selection-sort_624469?utm_source=striver&utm_medium=website&utm_campaign=a_zcoursetuf&leftPanelTab=1



void selectionSort(vector<int>&arr)
 {
     int n=arr.size();
    
   for(int i=0;i<n-1;i++)
    {
        for(int j=i+1;j<n;j++)
        {
           if(arr[i]>arr[j])
           {
               int temp= arr[i];
               arr[i]=arr[j];
               arr[j]=temp;
           }
        }
    }  
}
